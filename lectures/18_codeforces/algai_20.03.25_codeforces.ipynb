   "source": [
    "k = 9\n",
    "arr = [1, 3, 4, 5, 6, 10]\n",
    "\n",
    "i, j = 0, len(arr) - 1\n",
    "\n",
    "score = 0\n",
    "\n",
    "while(i < j):\n",
    "    el_sum = arr[i] + arr[j]\n",
    "    \n",
    "    if (el_sum == k):\n",
    "        score += 1\n",
    "        i += 1\n",
    "        j -= 1\n",
    "    \n",
    "    elif (el_sum > k):\n",
    "        j -= 1\n",
    "    \n",
    "    else:\n",
    "        i += 1\n",
    "\n",
    "print(score)"
   ]
